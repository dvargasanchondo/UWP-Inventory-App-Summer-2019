    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>


        <Button x:ConnectionId='2' x:Name="BackButton"                    Style="{StaticResource NavigationBackButtonNormalStyle}" RenderTransformOrigin="0.4,0.4" HorizontalAlignment="Left" VerticalAlignment="Top" />
